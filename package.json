{
  "name": "js-component-seed",
  "version": "0.0.1",
  "description": "A seed for writing javascript component",
  "main": "cjs/index.js",
  "scripts": {
    "start": "nodemon --exec yarn run dev",
    "dev": "cross-env NODE_ENV=production babel src -d demo/node_modules/js-component-seed --ignore *.test.js",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel src -d build --ignore *.test.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src -d build/es --ignore *.test.js",
    "build:copy-files": "babel-node ./script/copy-files.js",
    "build": "yarn prebuild && yarn build:es2015 && yarn build:es && yarn build:copy-files",
    "version": "yarn build && npm publish build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "doc": "jsdoc src -r"
  },
  "repository": "git@github.com:marcosun/js-component-seed.git",
  "author": "marcosun <marcoinbristol@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.10.0",
    "fs-extra": "^7.0.0",
    "jest": "^23.4.1",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2"
  },
  "files": [
    "lib",
    "cjs"
  ]
}
